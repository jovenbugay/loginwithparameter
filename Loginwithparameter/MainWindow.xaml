<Window x:Class="Loginwithparameter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Loginwithparameter"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="txtusername" HorizontalAlignment="Left" Height="23" Margin="178,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="txtpassword" HorizontalAlignment="Left" Margin="178,129,0,0" VerticalAlignment="Top" Width="120" Height="27"/>
        <Label x:Name="Status" Content="Status" HorizontalAlignment="Left" Margin="394,235,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Log In" HorizontalAlignment="Left" Margin="307,203,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
